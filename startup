ssh -N -f -L 8500:localhost:8500 ubuntu@...

sudo su && \

apt-get update -y && \

apt-get install -y unzip && \

cd /usr/bin && \

wget https://releases.hashicorp.com/consul/1.0.6/consul_1.0.6_linux_amd64.zip && \

unzip *.zip && rm -rf *.zip && \

mkdir -p /home/consul/consul_scripts && \

cd /home/consul/



